<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<automation uxui="external" userAgentType="tablet" testStrategy="Verify" 
	xsi:schemaLocation="http://www.skyve.org/xml/sail http://www.skyve.org/xml/sail.xsd" 
	xmlns="http://www.skyve.org/xml/sail" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interaction name="Menu DevOps::Data Maintenance">
        <method>
            <navigateEdit document="DataMaintenance" module="admin"/>
            <tabSelect path="Backup/Restore"/>
            <action name="Backup"/>
        </method>
    </interaction>
    <interaction name="Menu Jobs">
        <method>
            <navigateEdit document="Jobs" module="admin"/>
            <tabSelect path="Actual"/>
            <action name="Refresh"/>
        </method>
    </interaction>
</automation>