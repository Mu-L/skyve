<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<document name="NonPersistentAssociationToPersistent"
			xmlns="http://www.skyve.org/xml/document"
			xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<documentation>
		<![CDATA[
			Test if a transient association to a persistent document will cascade.
		]]>
	</documentation>
	<persistent name="TEST_NonPersistentAssociationToPersistent" />
	<singularAlias>NonPersistentAssociationToPersistent</singularAlias>
	<pluralAlias>NonPersistentAssociationToPersistent</pluralAlias>
	<bizKey expression="{text}" />
	<attributes>
		<text name="text" required="true">
			<displayName>Text</displayName>
			<length>255</length>
		</text>
		<association name="association" type="aggregation" persistent="false">
			<displayName>Association</displayName>
			<documentName>AllAttributesPersistent</documentName>
		</association>
	</attributes>
</document>
