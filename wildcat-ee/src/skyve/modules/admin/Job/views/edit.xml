<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<view title="{displayName} - {status}" type="edit" xmlns="http://www.skyve.org/xml/view" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<form border="true">
		<column percentageWidth="20" />
		<column />
		<column percentageWidth="20" />
		<column />
		<column />
		<row>
			<item colspan="3">
				<textField binding="displayName" disabled="true" />
			</item>
		</row>
		<row>
			<item colspan="3">
				<textField binding="startTime" disabled="true" />
			</item>
		</row>
		<row>
			<item colspan="3">
				<textField binding="endTime" disabled="true" />
			</item>
		</row>
		<row>
			<item>
				<textField binding="percentComplete" disabled="true" />
			</item>
			<item>
				<textField binding="status" disabled="true" />
			</item>
		</row>
	</form>
	<form border="true" borderTitle="Log">
		<column />
		<row>
			<item showLabel="false">
				<textArea binding="log" pixelHeight="500" />
			</item>
		</row>
	</form>
	<actions>
	</actions>
</view>

